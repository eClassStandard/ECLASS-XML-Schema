<?xml version="1.0" encoding="UTF-8"?>
<UnitsML xmlns="urn:oasis:names:tc:unitsml:schema:xsd:UnitsMLSchema-1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:unitsml:schema:xsd:UnitsMLSchema-1.0
UnitsML-v1.0-csd02.xsd">
	<UnitSet>
		<!-- MILLIMETER -->
		<Unit xml:id="IRDI_OF_MMT" dimensionURL="IRDI_OF_DIM1">
			<UnitName xml:lang="de">Millimeter</UnitName>
			<UnitName xml:lang="en">millimetre</UnitName>
			<UnitSymbol type="">mm</UnitSymbol>
			<CodeListValue codeListName="SI code" unitCodeValue="mm" />
			<CodeListValue codeListName="DIN code" unitCodeValue="mm" />
			<CodeListValue codeListName="ECE code" unitCodeValue="MMT" />
			<Conversions>
				<!-- y = f(x) = finalAddend + (multiplicand / divisor) (x + initialAddend) -->
				<Float64ConversionFrom initialUnit="m" xml:id="IRDI_OF_METER" divisor="1000" />
			</Conversions>
			<QuantityReference url="IRDI_OF_QUANTITY_1"/>
			<QuantityReference url="IRDI_OF_QUANTITY_2"/>
			<UnitDefinition xml:lang="de">0,001-faches der SI-Basiseinheit Meter</UnitDefinition>
			<UnitDefinition xml:lang="en">0,001 fold of the SI base unit metre</UnitDefinition>
		</Unit>
		
		<!-- lumen per watt -->
		<Unit xml:id="IRDI_OF_LMW" dimensionURL="IRDI_OF_M-2KG-1S3CDSR">
			<UnitName>Lumen Watt^-1</UnitName>
			<UnitSymbol type="">lm/W</UnitSymbol>
			<CodeListValue codeListName="SI code" unitCodeValue="lm/W" />
			<CodeListValue codeListName="SI name" unitCodeValue="lumen per watt" />
			<CodeListValue codeListName="DIN code" unitCodeValue="lm W-¹" />
			<CodeListValue codeListName="ECE name" unitCodeValue="ECE name lumen per watt" />
			<CodeListValue codeListName="ECE code" unitCodeValue="B61" /> 
			<CodeListValue codeListName="DIN name" unitCodeValue="Lumen durch Watt" />
			<!-- conversion factor - not needed???? -->			
			<UnitDefinition>abgeleitete SI-Einheit Lumen dividiert durch die abgeleitete SI-Einheit Watt</UnitDefinition>
			<UnitHistory>ISO 80000-7:2008 \ ISO 31-6:1992 \ ISO 1000:1992 \ DIN 1301-2:1978</UnitHistory>
		</Unit>
	</UnitSet>
	
	<QuantitySet>
		<Quantity xml:id="IRDI_OF_QUANTITY_1" dimensionURL="IRDI_OF_M-2KG-1S3CDSR">
			<QuantityName xml:lang="de">Lichtausbeute</QuantityName>
			<UnitReference url="IRDI_OF_LM/W"/>
			<UnitReference url="IRDI_OF_LM/W"/> <!-- can be multiple -->
			<QuantityDefinition xml:lang="de">Quotient aus abgestrahltem Lichtstrom und der zu seiner Erzeugung aufgewendeten elektrischer Leistung</QuantityDefinition>
		</Quantity>
	</QuantitySet>

	<DimensionSet>
		<Dimension xml:id="IRDI_OF_M-2KG-1S3CDSR"> <!--m-² kg-¹ s³ cd sr -->
			<Length powerDenominator="-2"  />
			<Mass powerDenominator="-1" />
			<Time powerDenominator="3" />
			<LuminousIntensity powerDenominator="1"/>
			<Item itemURL="sr" powerDenominator="1" />
		</Dimension>
	</DimensionSet>
</UnitsML>
